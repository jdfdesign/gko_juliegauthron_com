<%# collection_cache(parent) do %> 
<section class="page-header-section">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <div class="page-header">
          <%= render_page_title(parent.title) %>
          <%= render_slideshow_for(parent) %>
    	    <%= render_copy_text(parent.body) %>
    	  </div>
      </div>
    </div>
  </div>    
</section>
<section class="categories-section">
   <%= render "categories", :section => @portfolio %>
</section>
<section class="projects-section">
	<!-- Begin Projects -->
	<div class="container">
    <div id="projects-container" class="row">
      <%= render @projects %>
    </div> 
  </div>
</section> 
<%# end %>      
